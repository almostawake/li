  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Linked In Local",
      "cwd": "${workspaceFolder}/",
      "skipFiles": [
          "<node_internals>/**"
      ],
      "localRoot": "${workspaceFolder}/",
      "remoteRoot": "${workspaceFolder}/",
      "program": "${workspaceFolder}/src/index.ts",
      "outFiles": [
        "${workspaceFolder}/dist/**/*.js"
      ]
    }
  ]
}
